{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
           
            "name": "Launch nfpmwrapper",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "bazel-bin/go/cmd/nfpmwrapper/nfpmwrapper_/nfpmwrapper",
            "env": {},
            "cwd": "${workspaceFolder}",
            "args": [],
            "trace": "verbose",
            "substitutePath": [
                {
                    "from": "${workspaceFolder}/bazel-${workspaceFolderBasename}/external",
                    "to": "external/",
                },
                {
                    "from": "${workspaceFolder}/bazel-${workspaceFolderBasename}/external",
                    "to": "GOROOT/",
                },
                {
                    "from": "${workspaceFolder}/go",
                    "to": "go/",
                },
                {
                    "from": "${workspaceFolder}/go/internal",
                    "to": "go/internal/",
                },
            ],
            "logOutput": "debugger,dap",
            "showLog": true,
        },
    ]
}